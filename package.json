{
  "name": "obsidian-r2-sync",
  "version": "1.0.0",
  "description": "通过 Cloudflare R2 实现 Obsidian 多端同步的插件",
  "main": "main.js",
  "scripts": {
    "dev": "esbuild src/main.ts --bundle --external:obsidian --external:electron --outfile=main.js --format=cjs --platform=node --sourcemap",
    "build": "esbuild src/main.ts --bundle --external:obsidian --external:electron --outfile=main.js --format=cjs --platform=node --minify",
    "lint": "eslint src/main.ts"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "r2",
    "sync",
    "cloudflare"
  ],
  "author": "sandrone-muou",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.20.0",
    "eslint": "^10.0.0",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "obsidian": "latest",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.0.0"
}
